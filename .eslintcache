nents/Svgs/Smart.svg';\r\nimport { ReactComponent as Download} from '../components/Svgs/Download.svg';\r\nimport { ReactComponent as Free} from '../components/Svgs/Free.svg';\r\nimport { ReactComponent as Padlock} from '../components/Svgs/Padlock.svg';\r\nimport { ReactComponent as Profile} from '../components/Svgs/Profile.svg';\r\nimport { ReactComponent as Template} from '../components/Svgs/Template.svg';\r\n\r\n\r\n\r\n\r\nexport default [\r\n    {\r\n        icon: Smart,\r\n        header: 'Smart',\r\n        text: 'This tool saves you time and guides you to creating the best version of your resume.'\r\n    },\r\n    {\r\n        icon: Padlock,\r\n        header: 'Privacy Friendly',\r\n        text: 'We don’t store any of your data except you want to save progress.'\r\n    },\r\n    {\r\n        icon: Free,\r\n        header: '100% Free',\r\n        text: 'Use every feature for free without any hidden charge or limitations.'\r\n    },\r\n    {\r\n        icon: Template,\r\n        header: 'Multiple Templates',\r\n        text: 'There are multiple resume and cover letter templates for you to choose from.'\r\n    },\r\n    {\r\n        icon: Profile,\r\n        header: 'Multiple Profiles',\r\n        text: 'You can save your progress and create multiple profiles with different resumes.'\r\n    },\r\n    {\r\n        icon: Download,\r\n        header: 'Save Downloads',\r\n        text: 'After downloading your resume, you can save it for future downloads'\r\n    },\r\n    \r\n]","C:\\Users\\Femi\\Desktop\\resume-app-frontend\\src\\utilities\\history.js",[],"C:\\Users\\Femi\\Desktop\\resume-app-frontend\\src\\routes.jsx",[],"C:\\Users\\Femi\\Desktop\\resume-app-frontend\\src\\components\\SignUpPage\\index.jsx",[],"C:\\Users\\Femi\\Desktop\\resume-app-frontend\\src\\components\\AboutPage\\index.jsx",[],"C:\\Users\\Femi\\Desktop\\resume-app-frontend\\src\\components\\MobileMenuPage\\index.jsx",[],"C:\\Users\\Femi\\Desktop\\resume-app-frontend\\src\\components\\LoginPag